{
  "name": "contracts",
  "version": "0.0.0",
  "private": true,
  "license": "none",
  "scripts": {
    "gen": "yarn artifacts",
    "artifacts": "mkdir -p ./artifacts/wallet-contracts && yarn artifacts-copy:erc-1155 && yarn artifacts-copy:wallet-contracts && yarn generate",
    "artifacts-copy:erc-1155": "rm -rf ./artifacts/erc-1155 && cp -r ./node_modules/@0xsequence/erc-1155/artifacts/contracts ./artifacts/erc-1155",
    "artifacts-copy:wallet-contracts": "rm -rf ./artifacts/wallet-contracts/* && cp -r ./node_modules/@0xsequence/wallet-contracts/artifacts/contracts/* ./artifacts/wallet-contracts",
    "generate": "go generate ./gen"
  },
  "devDependencies": {
    "@0xsequence/erc-1155": "^3.0.4",
    "@0xsequence/wallet-contracts": "1.9.4"
  }
}
